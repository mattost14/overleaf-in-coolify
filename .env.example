# Optional environment variables for Overleaf
# Copy to .env and customize as needed

# Site Configuration
OVERLEAF_SITE_URL=http://localhost
OVERLEAF_NAV_TITLE=Overleaf
OVERLEAF_ADMIN_EMAIL=admin@example.com

# Behind Proxy (for Coolify/Traefik) - set to true when deploying to Coolify, false for local testing
OVERLEAF_BEHIND_PROXY=false

# Email Configuration (SMTP)
# OVERLEAF_EMAIL_FROM_ADDRESS=noreply@example.com
# OVERLEAF_EMAIL_SMTP_HOST=smtp.example.com
# OVERLEAF_EMAIL_SMTP_PORT=587
# OVERLEAF_EMAIL_SMTP_SECURE=false
# OVERLEAF_EMAIL_SMTP_USER=your-email@gmail.com
# OVERLEAF_EMAIL_SMTP_PASS=your-app-password
# Optional: EMAIL_CONFIRMATION_DISABLED=true

# File Upload Size Limits
# OVERLEAF_FILESTORE_MAX_FILE_SIZE_MB=50

# Compile Timeout
# OVERLEAF_COMPILE_TIMEOUT=60000

# Enable/Disable Features
# OVERLEAF_ALLOW_PUBLIC_ACCESS=true
# OVERLEAF_ALLOW_ANONYMOUS_READ_AND_WRITE_SHARING=true

# Custom Templates
# Create a dedicated templates user and get their user ID from the database
# All projects created by this user will appear as templates
# OVERLEAF_TEMPLATES_USER_ID=
# Add custom template links (JSON format)
# OVERLEAF_NEW_PROJECT_TEMPLATE_LINKS=[{"name":"NASA Templates","url":"https://github.com/nasa/nasa-latex-docs"}]
